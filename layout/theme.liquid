  </head>
  <body>

    {%-liquid  
  section 'announcement-bar'
  section 'header'-%}
    <main role="main">{{ content_for_layout }}</main>
    {%- section 'footer' -%}
    {% section 'app-optimization' %}
    {% render 'qr-tracking' %}
